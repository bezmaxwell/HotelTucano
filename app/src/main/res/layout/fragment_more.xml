<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:elevation="20sp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp">

        <TextView
            android:id="@+id/mais_fragment_more_nav_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Mais"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/atendimento_fragment_more_nav_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Atendimento"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mais_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/horario_atendimento_fragment_nav_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="24 horas, 7dias por semana!"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/atendimento_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/texViewTelefone_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Telefone"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/horario_atendimento_fragment_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:scaleType="center"
            android:src="@drawable/ic_baseline_smartphone_24"
            app:layout_constraintRight_toRightOf="@+id/texViewTelefone_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/texViewTelefone_fragment_more_nav_bar" />


        <TextView
            android:id="@+id/atendimentoOnline_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Atendimento Online"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/texViewTelefone_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_chat_bubble_outline_24"
            app:layout_constraintRight_toRightOf="@+id/atendimentoOnline_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/atendimentoOnline_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/email_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="E-mail"
            android:textSize="17sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/atendimentoOnline_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_email_24"
            app:layout_constraintRight_toRightOf="@+id/email_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/email_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/textViewSocial_fragment_more_nav_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30sp"
            android:text="Social"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/email_fragment_more_nav_bar" />


        <TextView
            android:id="@+id/avalie_nosso_app_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Avalie nosso app"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/textViewSocial_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_star_24"
            app:layout_constraintRight_toRightOf="@+id/avalie_nosso_app_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/avalie_nosso_app_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/blog_do_viajante_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Blog do viajante- GoCano"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/avalie_nosso_app_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_flight_takeoff_24"
            app:layout_constraintRight_toRightOf="@id/blog_do_viajante_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/blog_do_viajante_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/pagina_do_facebook_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Pagina do facebook"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/blog_do_viajante_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_sms_24"
            app:layout_constraintRight_toRightOf="@id/pagina_do_facebook_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/pagina_do_facebook_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/pagina_do_instagram_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Pagina do Instagram"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/pagina_do_facebook_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_camera_alt_24"
            app:layout_constraintRight_toRightOf="@id/pagina_do_instagram_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/pagina_do_instagram_fragment_more_nav_bar" />


        <TextView
            android:id="@+id/sobre_fragment_more_nav_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Sobre"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/pagina_do_instagram_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/termos_de_uso_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Termos de uso"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/sobre_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_menu_book_24"
            app:layout_constraintRight_toRightOf="@id/termos_de_uso_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/termos_de_uso_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/politicas_de_privacidade_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Políticas de privacidade"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/termos_de_uso_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_lock_24"
            app:layout_constraintRight_toRightOf="@id/politicas_de_privacidade_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/politicas_de_privacidade_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/licencas_open_source_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
            android:text="Licenças Open Source"
            android:textSize="17sp"
            app:layout_constraintTop_toBottomOf="@+id/politicas_de_privacidade_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_book_24"
            app:layout_constraintRight_toRightOf="@id/licencas_open_source_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/licencas_open_source_fragment_more_nav_bar" />


        <TextView
            android:id="@+id/configuracoes_fragment_more_nav_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Configurações"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/licencas_open_source_fragment_more_nav_bar" />

        <TextView
            android:id="@+id/regiao_idioma_moeda_fragment_more_nav_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Região, idioma e moeda"
            android:textSize="20sp"
            app:layout_constraintTop_toBottomOf="@+id/configuracoes_fragment_more_nav_bar" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_settings_24"
            app:layout_constraintRight_toRightOf="@id/regiao_idioma_moeda_fragment_more_nav_bar"
            app:layout_constraintTop_toTopOf="@+id/regiao_idioma_moeda_fragment_more_nav_bar" />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp"
            android:text="TUC \u00a9 2020 * Versão 0.0.1 "
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/regiao_idioma_moeda_fragment_more_nav_bar" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
